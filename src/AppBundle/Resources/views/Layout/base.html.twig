<!DOCTYPE html>
<html class="no-js" lang="en">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="x-ua-compatible" content="ie=edge">
        <title>{% block name %}emerge{% endblock %}</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="robots" content="NOODP">
        <link rel="apple-touch-icon" href="apple-touch-icon.png">

        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('/bundles/app/assets/vendor/bootstrap-3.3.7-dist/css/bootstrap.min.css') }}">
            <link rel="stylesheet" href="{{ asset('/bundles/app/assets/vendor/font-awesome-4.7.0/css/font-awesome.min.css') }}">
        {% endblock %}
        {% block head %}{% endblock %}
    </head>
    {% block body_attributes %}
        <body>
    {% endblock %}
        <!--[if lte IE 9]>
            <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience and security.</p>
        <![endif]-->

        {% block body %} {% endblock %}

        {% block javascripts %}
            <script src="https://code.jquery.com/jquery-3.2.0.min.js" crossorigin="anonymous"></script>
            <script>window.jQuery || document.write('<script src="{{ asset('/bundles/app/assets/js/vendor/jquery/jquery-3.2.0.min.js') }}"><\/script>')</script>
            <script src="{{ asset('/bundles/app/assets/vendor/bootstrap-3.3.7-dist/js/bootstrap.min.js') }}"></script>
            <script src="{{ asset('/bundles/app/assets/js/plugins.js') }}"></script>
            <script src="{{ asset('/bundles/app/assets/js/dashboard.js') }}"></script>
        {% endblock %}

    </body>
</html>
