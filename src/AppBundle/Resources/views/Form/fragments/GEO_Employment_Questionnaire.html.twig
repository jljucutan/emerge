    $("input[type=text]").addClass('lifesuite__text-input');
    $("input[type=text].hasDatepicker").addClass('dateField');
    $("select").addClass('lifesuite__select-input');
    $("input[type=radio]").addClass('lifesuite__radio');
    $("input[type=checkbox]").addClass('lifesuite__checkbox');
    $("textarea").addClass('lifesuite__textarea-input');
    $('label').addClass('lifesuite__input-label lifesuite__input—block');
});

$(document).on('ready', function() {
    "use strict";
    $(".modal").hide();
    $('[data-checkbox-group]').on('click', function() {
        let groupId = $(this).data('checkbox-group');
        let checked = $(this).prop('checked');
        $('[data-checkbox-group="' + groupId + '"]').prop('checked', false);
        $(this).prop('checked', checked);
    });
    // sets responsive table uniform height
    const setCommonHeight = function() {
        $('.common-height').each(function() {
            let height = $(this).height();
            $.each($(this).children('div'), function(k,v) {
                $(v).css({'height': height});
            });
        });
    };
    if(!/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
        setCommonHeight();
    }
    $(window).on('resize', function() {
        location.reload();
    });
    // inputs only accepts valid telephone number
    $('.phone-dashes').on('keyup paste', function() {
        let num = $(this).val().replace(/\D/g, '').match(/(\d{3})(\d{3})(\d{4})/);
        if (num) {
            $(this).val(num[1] + '-' + num[2] + '-' + num[3]);
        }
    });

    // show toggleable sections that has been filled out
    $.each($('[data-toggleable-section] input[type="text"], [data-toggleable-section] select'), function(k,v) {
        if ($(v).val().length > 0) {
            console.log($(v).prop('name') + ' ' + $(v).val())
            $(v).closest('[data-toggleable-section]').show();
        }
    });

    // toggle sections
    const showSections = function(controller, target) {
        if (target.length > 0) {
            $(target).first().show();
        }
    }
    const hideSections = function(controller, target) {
        if (target.length > 0) {
            $(target).last().hide();
        }
    }
    const toggleSectionControls = function(section, target) {
        let hasTarget = false;
        let targets = 0;
        $.each(target, function(k,v) {
            if ($(v).is(':visible')) {
                hasTarget = true;
                targets += 1;
            }
        });
        if (!hasTarget) {
            section.find('[data-control="delete"]').hide();
        }
        if (hasTarget) {
            section.find('[data-control="delete"]').show();
        }
        if (targets >= target.length) {
            section.find('[data-control="add"]').hide();
            return;
        }
        section.find('[data-control="add"]').show();
    }
    $('[data-control-section] button').on('click', function() {
        let sectionGroup = $(this).parent().data('control-section');
        switch ($(this).data('control')) {
            case "add":
            let sectionHidden = $('[data-toggleable-section="' + sectionGroup + '"]:hidden');
            showSections($(this), sectionHidden);
            break;
            case "delete":
            let sectionVisible = $('[data-toggleable-section="' + sectionGroup + '"]:visible');
            hideSections($(this), sectionVisible);
            break;
        }
        toggleSectionControls($(this).parent(), $('[data-toggleable-section="' + sectionGroup + '"]'));
    });
    toggleSectionControls($('[data-control-section="police-record"]'), $('[data-toggleable-section="police-record"]'));
    toggleSectionControls($('[data-control-section="post-conviction-jobs"]'), $('[data-toggleable-section="post-conviction-jobs"]'));
    toggleSectionControls($('[data-control-section="confinement-facility-jobs"]'), $('[data-toggleable-section="confinement-facility-jobs"]'));
    if (strFormCompleted) {
        $('[data-control]')
            .removeClass('alt-primary primary')
            .addClass('disabled')
            .prop('disabled', 'disabled');
    }
});
</script>
