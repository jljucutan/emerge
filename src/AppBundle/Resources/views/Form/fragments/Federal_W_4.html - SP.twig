const W4_disableCalButton = function(el) {
    "use strict";
    el.parent().find('input').prop('disabled', 'disabled');
}
const getSumFor = function(el) {
    "use strict";
    if (!el.hasAttribute('data-addends')) {
        return false;
    }
    let addends = el.getAttribute('data-addends').split(',');
    let sum = 0;
    for (let i = 0; i < addends.length; i++) {
        let nodeValue = Number(document.querySelector('[name$=".' + addends[i] + '"]').value.replace(/[^0-9.-]+/g,""));
        if (isNaN(nodeValue) == true) {
            nodeValue = 0;
        }
        sum = sum + nodeValue;
    }
    el.value = sum;
}

$(document).on('ready', function() {
    $('#MultiplyChildren, #MultiplyDependents').on('keyup paste', function() {
        getSumFor(document.querySelector('#ChildrenDependentsTotal'));
    });
    $('#daworksheet3, #daworksheet4').on('keyup paste', function() {
        getSumFor(document.querySelector('#daworksheet5'));
        getSumFor(document.querySelector('#Deductions'));
    });
    $('#MJWorksheet2A, #MJWorksheet2B').on('keyup paste', function() {
        getSumFor(document.querySelector('#MJWorksheet2C'));
    });

    // remove non numeric
    $('[name$=".MultiplyChildren"], [name$=".MultiplyDependents"], [name$=".OtherIncome"], [name$=".Deductions"], [name$=".ExtraWithholding"]').on('keyup change paste', function() {
        $(this).val($(this).val().replace(/[^0-9]/gi,""));
    });
    $('[name$=".MultiplyChildren"], [name$=".MultiplyDependents"], [name$=".OtherIncome"], [name$=".Deductions"], [name$=".ExtraWithholding"]').trigger("change");

    W4_disableCalButton($('.disabled-date'));

    $(".modal").hide();
});
