                "regex": "^[0-9]{11}$",
                "message": "11 digits"
              },
              "MEX-RFC": {
                "validation": "regex-optional,required",
                "regex": "^[A-Z]{4}[0-9]{6}[A-Z0-9]{3}$",
                "message": "13 character (four upper-case letter, followed by 6 digits and ending in 3 alphanumeric characters)"
              },
              "USA-SSN": {
